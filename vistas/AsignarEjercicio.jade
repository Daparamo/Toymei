  body
    style.
      body {      
      overflow: hidden;
      }

    center
      #header
        span.titulo Asignar Ejercicio
        #ButtonHeader
          a(href='/')
            img#atras.iconHeader(src='img/Atras.png', border='0')          
          a(href='/logout')
            img#logout.iconHeader(src='img/exit.png', border='0')
        br
        select#TipoEjercicio(name='TipoEjercicio')
          option(value='0') Ejercicio
          option(value='1') Estiramiento
        input#TiempoEjercicio(name='TiempoEjercicio', type='number', min='0', placeholder='Tiempo en segundos')
        input#RepeticionesEjercicio(name='RepeticionesEjercicio', min='0', type='number', placeholder='Numero de repeticiones')
